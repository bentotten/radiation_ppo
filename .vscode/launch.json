  "version": "0.2.0",
  "configurations": [
    {
      "name": "Build Docs",
      "type": "bashdb",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/build_docs.sh",
      "args": []
    },
    {
      "name": "RAD-A2C",
      "type": "python",
      //"python": "${command:python.interpreterPath}/../multi_ppo_torch", # Specify which interpreter
      "request": "launch",
      "program": "${workspaceFolder}/algos/ppo/main.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "args": ["--debug", "True", "--epochs", "25", "--render", "True"],
      "cwd": "${workspaceFolder}/algos/ppo"
    },
    {
      "name": "PLOT MULTI",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/algos/multiagent/plot_results.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "args": ["--data_dir", "${workspaceFolder}/models", "--smooth", "10"],
      "cwd": "${workspaceFolder}/algos/multiagent"
    },
    {
      "name": "Eval MULTI",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/algos/multiagent/main.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "args": [
        "--mode",
        "evaluate",
        "--exp-name",
        "RAD-TEAM",
        "--obstruct",
        "0",
        "--agent-count",
        "1",
        "--net-type",
        "og",
        // "--seed",
        // "2",
        "--no-enforce-boundaries",
        "--no-global-critic",
        // "--render",
        // "True",
        // "--save-gif-freq",
        // "20"
      ],
      "cwd": "${workspaceFolder}/algos/multiagent"
    },
    {
      "name": "Train MULTI",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/algos/multiagent/main.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "args": [
        "--agent-count",
        "1",
        "--steps-per-epoch",
        "480",
        "--steps-per-episode",
        "120",
        "--epochs",
        "2",
        // "--render",
        // "True",
        // "--save-gif-freq",
        // "10",
        "--obstruct",
        "0",
        // "--seed",
        // "2",
        // "--exp-name",
        // "TEAM-RAD",
        "--net-type",
        "cnn",
        "--no-enforce-boundaries"
        // "--DEBUG",
        // "True",
        // "--save-freq",
        // "25",
        //"--no-global-critic",
        // "--dims",
        // "1200",
        // "1200",
        // "--area-obs",
        // "0.0",
        // "0.0"
      ],
      "cwd": "${workspaceFolder}/algos/multiagent"
    },
    {
      "name": "Hail Mary - Train OG project w/MARL env",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/algos/test_environment/ppo.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "args": [],
      "cwd": "${workspaceFolder}/algos/test_environment"
    },
    {
      "name": "Hail Mary - Eval OG project w/MARL env",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/algos/test_environment/eval/test_policy.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "args": ["--fpath", "${workspaceFolder}/models/train/pre_train/rad_a2c/loc24_hid24_pol32_val32_alpha01_tkl07_val01_lam09_npart40_lr3e-4_proc10_obs-1_iter40_blr5e-3_2_tanh_ep3000_steps4800_s1"],
      "cwd": "${workspaceFolder}/algos/test_environment"
    }       
  ]
}
